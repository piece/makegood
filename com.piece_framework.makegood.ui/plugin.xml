<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="com.piece_framework.makegood.ui.definitions.phpContentType">
         <with
               variable="activeContexts">
            <iterate
                  operator="or">
               <equals
                     value="org.eclipse.php.core.phpsource">
               </equals>
            </iterate>
         </with>
      </definition>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="com.piece_framework.makegood.ui.views.makeGoodCategory"
            name="%views.makeGoodCategory.name">
      </category>
      <view
            category="com.piece_framework.makegood.ui.views.makeGoodCategory"
            class="com.piece_framework.makegood.ui.views.ResultView"
            icon="icons/makegood.gif"
            id="com.piece_framework.makegood.ui.views.resultView"
            name="%views.resultView.name"
            restorable="true">
      </view>
      <view
            category="com.piece_framework.makegood.ui.views.makeGoodCategory"
            class="com.piece_framework.makegood.ui.views.TestOutlineView"
            icon="icons/testOutlineView.gif"
            id="com.piece_framework.makegood.ui.views.testOutlineView"
            name="Test Outline"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <separator
               name="Run tests separator"
               visible="true">
         </separator>
         <command
               commandId="com.piece_framework.makegood.ui.commands.runTestFromExplorerCommand"
               icon="icons/makegood.gif"
               label="%commands.runTestFromExplorerCommand.label"
               style="push">
            <visibleWhen
                  checkEnabled="true">
               <with
                     variable="activeMenuSelection">
                  <and>
                     <or>
                        <iterate
                              ifEmpty="false"
                              operator="and">
                           <or>
                              <adapt
                                    type="org.eclipse.core.resources.IFolder">
                              </adapt>
                              <adapt
                                    type="org.eclipse.core.resources.IFile">
                              </adapt>
                              <adapt
                                    type="org.eclipse.dltk.core.IProjectFragment">
                              </adapt>
                              <adapt
                                    type="org.eclipse.dltk.core.IScriptFolder">
                              </adapt>
                              <adapt
                                    type="org.eclipse.dltk.core.IModule">
                              </adapt>
                              <adapt
                                    type="org.eclipse.dltk.core.IType">
                              </adapt>
                           </or>
                        </iterate>
                        <iterate
                              ifEmpty="false"
                              operator="and">
                           <adapt
                                 type="org.eclipse.dltk.core.IMethod">
                           </adapt>
                        </iterate></or>
                     <count
                           value="+">
                     </count>
                  </and>
               </with>
            </visibleWhen>
         </command>
         <menu
               icon="icons/makegood.gif"
               label="%menu.runTestsFor.label">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="com.piece_framework.makegood.ui.definitions.phpContentType">
               </reference>
            </visibleWhen>
            <command
                  commandId="com.piece_framework.makegood.ui.commands.runTestFromEditorInContextCommand"
                  label="%commands.runTestFromEditorInContextCommand.label"
                  style="push">
            </command>
            <command
                  commandId="com.piece_framework.makegood.ui.commands.runTestFromEditorInClassCommand"
                  label="%commands.runTestFromEditorInClassCommand.label"
                  style="push">
            </command>
            <command
                  commandId="com.piece_framework.makegood.ui.commands.runTestFromEditorInFileCommand"
                  label="%commands.runTestFromEditorInFileCommand.label"
                  style="push">
            </command>
         </menu>
         <command
               commandId="com.piece_framework.makegood.ui.commands.runRelatedTestFromEditorCommand"
               icon="icons/makegood.gif"
               label="%commands.runRelatedTestFromEditorCommand.label"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="com.piece_framework.makegood.ui.definitions.phpContentType">
               </reference>
            </visibleWhen>
         </command>
         <command
               commandId="com.piece_framework.makegood.ui.commands.runAllTestsCommand"
               icon="icons/makegood.gif"
               label="%commands.runAllTestsCommand.label"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.trim.status">
         <toolbar
               id="com.piece_framework.makegood.ui.widgets.statusToolbar">
            <control
                  class="com.piece_framework.makegood.ui.widgets.ResultSquare"
                  id="com.piece_framework.makegood.ui.widgets.resultSquare">
            </control>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="com.piece_framework.makegood.ui.commands.runTestCategory"
            name="%commands.runTestCategory.name">
      </category>
      <command
            categoryId="com.piece_framework.makegood.ui.commands.runTestCategory"
            defaultHandler="com.piece_framework.makegood.ui.handlers.RunTestFromExplorerHandler"
            id="com.piece_framework.makegood.ui.commands.runTestFromExplorerCommand"
            name="%commands.runTestFromExplorerCommand.name">
      </command>
      <command
            categoryId="com.piece_framework.makegood.ui.commands.runTestCategory"
            defaultHandler="com.piece_framework.makegood.ui.handlers.RunTestFromEditorHandlerInContext"
            id="com.piece_framework.makegood.ui.commands.runTestFromEditorInContextCommand"
            name="%commands.runTestFromEditorInContextCommand.name">
      </command>
      <command
            categoryId="com.piece_framework.makegood.ui.commands.runTestCategory"
            defaultHandler="com.piece_framework.makegood.ui.handlers.RunTestFromEditorInClassHandler"
            id="com.piece_framework.makegood.ui.commands.runTestFromEditorInClassCommand"
            name="%commands.runTestFromEditorInClassCommand.name">
      </command>
      <command
            categoryId="com.piece_framework.makegood.ui.commands.runTestCategory"
            defaultHandler="com.piece_framework.makegood.ui.handlers.RunTestFromEditorInFileHandler"
            id="com.piece_framework.makegood.ui.commands.runTestFromEditorInFileCommand"
            name="%commands.runTestFromEditorInFileCommand.name">
      </command>
      <command
            categoryId="com.piece_framework.makegood.ui.commands.runTestCategory"
            defaultHandler="com.piece_framework.makegood.ui.handlers.RunRelatedTestFromEditorHandler"
            id="com.piece_framework.makegood.ui.commands.runRelatedTestFromEditorCommand"
            name="%commands.runRelatedTestFromEditorCommand.name">
      </command>
      <command
            categoryId="com.piece_framework.makegood.ui.commands.runTestCategory"
            defaultHandler="com.piece_framework.makegood.ui.handlers.RunAllTestsHandler"
            id="com.piece_framework.makegood.ui.commands.runAllTestsCommand"
            name="%commands.runAllTestsCommand.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="com.piece_framework.makegood.ui.commands.resultViewCategory"
            name="%commands.resultViewCategory.name">
      </category>
      <command
            categoryId="com.piece_framework.makegood.ui.commands.resultViewCategory"
            defaultHandler="com.piece_framework.makegood.ui.handlers.MoveToNextFailureHandler"
            id="com.piece_framework.makegood.ui.commands.nextFailureCommand"
            name="%commands.nextFailureCommand.name">
      </command>
      <command
            categoryId="com.piece_framework.makegood.ui.commands.resultViewCategory"
            defaultHandler="com.piece_framework.makegood.ui.handlers.MoveToPreviousFailureHandler"
            id="com.piece_framework.makegood.ui.commands.previousFailureCommand"
            name="%commands.previousFailureCommand.name">
      </command>
      <command
            categoryId="com.piece_framework.makegood.ui.commands.resultViewCategory"
            defaultHandler="com.piece_framework.makegood.ui.handlers.ToggleShowOnlyFailuresHandler"
            id="com.piece_framework.makegood.ui.commands.showOnlyFailuresCommand"
            name="%commands.showOnlyFailuresCommand.name">
      </command>
      <command
            categoryId="com.piece_framework.makegood.ui.commands.resultViewCategory"
            defaultHandler="com.piece_framework.makegood.ui.handlers.SwitchToUnselectedTestResultsTabHandler"
            id="com.piece_framework.makegood.ui.commands.switchToUnselectedTestResultsTabCommand"
            name="%commands.switchToUnselectedTestResultsTabCommand.name">
      </command>
      <command
            categoryId="com.piece_framework.makegood.ui.commands.runTestCategory"
            defaultHandler="com.piece_framework.makegood.ui.handlers.ToggleStopOnFailureHandler"
            id="com.piece_framework.makegood.ui.commands.stopOnFailureCommand"
            name="%commands.stopOnFailureCommand.name">
      </command>
      <command
            categoryId="com.piece_framework.makegood.ui.commands.runTestCategory"
            defaultHandler="com.piece_framework.makegood.ui.handlers.ToggleDebugTestHandler"
            id="com.piece_framework.makegood.ui.commands.debugTestCommand"
            name="%commands.debugTestCommand.name">
      </command>
      <command
            categoryId="com.piece_framework.makegood.ui.commands.runTestCategory"
            defaultHandler="com.piece_framework.makegood.ui.handlers.StopTestHandler"
            id="com.piece_framework.makegood.ui.commands.stopTestCommand"
            name="%commands.stopTestCommand.name">
      </command>
      <command
            categoryId="com.piece_framework.makegood.ui.commands.runTestCategory"
            defaultHandler="com.piece_framework.makegood.ui.handlers.RerunFailedTestsHandler"
            id="com.piece_framework.makegood.ui.commands.rerunFailedTestsCommand"
            name="%commands.rerunFailedTestsCommand.name">
      </command>
      <command
            categoryId="com.piece_framework.makegood.ui.commands.runTestCategory"
            defaultHandler="com.piece_framework.makegood.ui.handlers.RerunTestHandler"
            id="com.piece_framework.makegood.ui.commands.rerunTestCommand"
            name="%commands.rerunTestCommand.name">
      </command>
      <command
            categoryId="com.piece_framework.makegood.ui.commands.resultViewCategory"
            defaultHandler="com.piece_framework.makegood.ui.handlers.ToggleContinuousTestingHandler"
            id="com.piece_framework.makegood.ui.commands.toggleContinuousTestingCommand"
            name="%commands.toggleContinuousTestingCommand.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            class="com.piece_framework.makegood.ui.preferences.MakeGoodPropertyPage"
            id="com.piece_framework.makegood.ui.propertyPages.makeGood"
            name="%propertyPages.makeGood.name">
         <enabledWhen>
            <or>
               <instanceof
                     value="org.eclipse.core.resources.IProject">
               </instanceof>
               <instanceof
                     value="org.eclipse.dltk.core.IScriptProject">
               </instanceof>
            </or>
         </enabledWhen>
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.contexts">
      <context
            id="com.piece_framework.makegood.ui.contexts.resultView"
            name="%contexts.resultView.name">
      </context>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="com.piece_framework.makegood.ui.commands.nextFailureCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M J">
      </key>
      <key
            commandId="com.piece_framework.makegood.ui.commands.previousFailureCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M K">
      </key>
      <key
            commandId="com.piece_framework.makegood.ui.commands.showOnlyFailuresCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M O">
      </key>
      <key
            commandId="com.piece_framework.makegood.ui.commands.rerunTestCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M R">
      </key>
      <key
            commandId="com.piece_framework.makegood.ui.commands.runTestFromEditorInContextCommand"
            contextId="org.eclipse.php.core.phpsource"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M C">
      </key>
      <key
            commandId="com.piece_framework.makegood.ui.commands.runAllTestsCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M A">
      </key>
      <key
            commandId="com.piece_framework.makegood.ui.commands.runTestFromExplorerCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M X">
      </key>
      <key
            commandId="com.piece_framework.makegood.ui.commands.runTestFromEditorInClassCommand"
            contextId="org.eclipse.php.core.phpsource"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M L">
      </key>
      <key
            commandId="com.piece_framework.makegood.ui.commands.runTestFromEditorInFileCommand"
            contextId="org.eclipse.php.core.phpsource"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M F">
      </key>
      <key
            commandId="com.piece_framework.makegood.ui.commands.runRelatedTestFromEditorCommand"
            contextId="org.eclipse.php.core.phpsource"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M D">
      </key>
      <key
            commandId="com.piece_framework.makegood.ui.commands.stopOnFailureCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M E">
      </key>
      <key
            commandId="com.piece_framework.makegood.ui.commands.stopTestCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M S">
      </key>
      <key
            commandId="com.piece_framework.makegood.ui.commands.debugTestCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M B">
      </key>
      <key
            commandId="com.piece_framework.makegood.ui.commands.switchToUnselectedTestResultsTabCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M T">
      </key>
      <key
            commandId="com.piece_framework.makegood.ui.commands.rerunFailedTestsCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M G">
      </key>
      <key
            commandId="com.piece_framework.makegood.ui.commands.toggleContinuousTestingCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+M @">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.viewActions">
      <viewContribution
            id="com.piece_framework.makegood.ui.viewActions.resultView"
            targetID="com.piece_framework.makegood.ui.views.resultView">
         <action
               class="com.piece_framework.makegood.ui.actions.ConfigureContinuousTestingAction"
               definitionId="com.piece_framework.makegood.ui.commands.toggleContinuousTestingCommand"
               icon="icons/configure_continuous_testing_enabled.gif"
               id="com.piece_framework.makegood.ui.viewActions.configureContinuousTestingAction"
               label="%viewActions.configureContinuousTestingAction.label"
               style="pulldown"
               toolbarPath="continuoustesting"
               tooltip="%viewActions.configureContinuousTestingAction.tooltip">
         </action>
         <action
               class="com.piece_framework.makegood.ui.actions.StopTestAction"
               definitionId="com.piece_framework.makegood.ui.commands.stopTestCommand"
               icon="icons/stopTestAction.gif"
               id="com.piece_framework.makegood.ui.viewActions.stopTestAction"
               label="%viewActions.stopTestAction.label"
               style="push"
               toolbarPath="runs"
               tooltip="%viewActions.stopTestAction.tooltip">
         </action>
         <action
               class="com.piece_framework.makegood.ui.actions.RerunFailedTestsAction"
               definitionId="com.piece_framework.makegood.ui.commands.rerunFailedTestsCommand"
               icon="icons/rerun_failed_tests.gif"
               id="com.piece_framework.makegood.ui.viewActions.rerunFailedTestsAction"
               label="%viewActions.rerunFailedTestsAction.label"
               style="push"
               toolbarPath="runs"
               tooltip="%viewActions.rerunFailedTestsAction.tooltip">
         </action>
         <action
               class="com.piece_framework.makegood.ui.actions.RerunTestAction"
               definitionId="com.piece_framework.makegood.ui.commands.rerunTestCommand"
               icon="icons/rerun_test.gif"
               id="com.piece_framework.makegood.ui.viewActions.rerunTestAction"
               label="%viewActions.rerunTestAction.label"
               style="push"
               toolbarPath="runs"
               tooltip="%viewActions.rerunTestAction.tooltip">
         </action>
         <action
               class="com.piece_framework.makegood.ui.actions.RunAllTestsAction"
               definitionId="com.piece_framework.makegood.ui.commands.runAllTestsCommand"
               icon="icons/run_all_tests.gif"
               id="com.piece_framework.makegood.ui.viewActions.runAllTestsAction"
               label="%viewActions.runAllTestsAction.label"
               style="push"
               toolbarPath="runs"
               tooltip="%viewActions.runAllTestsAction.tooltip">
         </action>
         <action
               class="com.piece_framework.makegood.ui.actions.ToggleStopOnFailureAction"
               definitionId="com.piece_framework.makegood.ui.commands.stopOnFailureCommand"
               icon="icons/toggle_stop_on_failure.gif"
               id="com.piece_framework.makegood.ui.viewActions.toggleStopOnFailureAction"
               label="%viewActions.toggleStopOnFailureAction.label"
               style="toggle"
               toolbarPath="properties"
               tooltip="%viewActions.toggleStopOnFailureAction.tooltip">
         </action>
         <action
               class="com.piece_framework.makegood.ui.actions.ToggleDebugTestAction"
               definitionId="com.piece_framework.makegood.ui.commands.debugTestCommand"
               icon="icons/toggle_debug_test.gif"
               id="com.piece_framework.makegood.ui.viewActions.toggleDebugTestAction"
               label="%viewActions.toggleDebugTestAction.label"
               style="toggle"
               toolbarPath="properties"
               tooltip="%viewActions.toggleDebugTestAction.tooltip">
         </action>
         <action
               class="com.piece_framework.makegood.ui.actions.ToggleShowOnlyFailuresAction"
               definitionId="com.piece_framework.makegood.ui.commands.showOnlyFailuresCommand"
               icon="icons/toggle_show_only_failures.gif"
               id="com.piece_framework.makegood.ui.viewActions.toggleShowOnlyFailuresAction"
               label="%viewActions.toggleShowOnlyFailuresAction.label"
               style="toggle"
               toolbarPath="results"
               tooltip="%viewActions.toggleShowOnlyFailuresAction.tooltip">
         </action>
         <action
               class="com.piece_framework.makegood.ui.actions.MoveToPreviousFailureAction"
               definitionId="com.piece_framework.makegood.ui.commands.previousFailureCommand"
               icon="icons/move_to_previous_failure.gif"
               id="com.piece_framework.makegood.ui.viewActions.moveToPreviousFailureAction"
               label="%viewActions.moveToPreviousFailureAction.label"
               style="push"
               toolbarPath="results"
               tooltip="%viewActions.moveToPreviousFailureAction.tooltip">
         </action>
         <action
               class="com.piece_framework.makegood.ui.actions.MoveToNextFailureAction"
               definitionId="com.piece_framework.makegood.ui.commands.nextFailureCommand"
               icon="icons/move_to_next_failure.gif"
               id="com.piece_framework.makegood.ui.viewActions.moveToNextFailureAction"
               label="%viewActions.moveToNextFailureAction.label"
               style="push"
               toolbarPath="results"
               tooltip="%viewActions.moveToNextFailureAction.tooltip">
         </action>
      </viewContribution>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="com.piece_framework.makegood.ui.MakeGoodPreparer">
      </startup>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="com.piece_framework.makegood.ui.preferences.MakeGoodPreferencePage"
            id="com.piece_framework.makegood.ui.preferencePages.makeGood"
            name="%preferencePages.makeGood.name">
      </page>
   </extension>
   <extension
         id="com.piece_framework.makegood.ui.markers.testMarker"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.eclipse.core.resources.problemmarker">
      </super>
      <super
            type="org.eclipse.core.resources.textmarker">
      </super>
      <persistent
            value="false">
      </persistent>
   </extension>
   <extension
         id="com.piece_framework.makegood.ui.markers.testFailureMarker"
         name="MakeGood Test Failure"
         point="org.eclipse.core.resources.markers">
      <super
            type="com.piece_framework.makegood.ui.markers.testMarker">
      </super>
   </extension>
   <extension
         id="com.piece_framework.makegood.ui.markers.testErrorMarker"
         name="MakeGood Test Error"
         point="org.eclipse.core.resources.markers">
      <super
            type="com.piece_framework.makegood.ui.markers.testMarker">
      </super>
   </extension>
   <extension
         id="com.piece_framework.makegood.ui.markers.fatalErrorMarker"
         name="MakeGood Fatal Error"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.eclipse.core.resources.problemmarker">
      </super>
      <super
            type="org.eclipse.core.resources.textmarker">
      </super>
      <persistent
            value="false">
      </persistent>
   </extension>
   <extension
         point="org.eclipse.ui.editors.annotationTypes">
      <type
            markerType="com.piece_framework.makegood.ui.markers.testMarker"
            name="com.piece_framework.makegood.ui.markers.testMarkerAnnotation"
            super="org.eclipse.ui.workbench.texteditor.error">
      </type>
      <type
            markerType="com.piece_framework.makegood.ui.markers.testFailureMarker"
            name="com.piece_framework.makegood.ui.markers.testFailureMarkerAnnotation"
            super="com.piece_framework.makegood.ui.markers.testMarkerAnnotation">
      </type>
      <type
            markerType="com.piece_framework.makegood.ui.markers.testErrorMarker"
            name="com.piece_framework.makegood.ui.markers.testErrorMarkerAnnotation"
            super="com.piece_framework.makegood.ui.markers.testMarkerAnnotation">
      </type>
   </extension>
   <extension
         point="org.eclipse.ui.editors.markerAnnotationSpecification">
      <specification
            annotationType="com.piece_framework.makegood.ui.markers.testFailureMarkerAnnotation"
            icon="icons/failure_gray.gif">
      </specification>
      <specification
            annotationType="com.piece_framework.makegood.ui.markers.testErrorMarkerAnnotation"
            icon="icons/error_gray.gif">
      </specification>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.php.perspective">
         <view
               id="com.piece_framework.makegood.ui.views.testOutlineView"
               minimized="false"
               relationship="stack"
               relative="org.eclipse.ui.views.ContentOutline"
               visible="true">
         </view>
      </perspectiveExtension>
   </extension>

</plugin>
